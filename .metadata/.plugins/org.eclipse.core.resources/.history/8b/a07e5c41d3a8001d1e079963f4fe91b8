
public class ListasDuplamenteEncadeadas <T>{

	
	private No<T> primeitroNo;
	private No<T> ultimoNo;
	
	private int tamanhoLista = 0;
	
	public ListasDuplamenteEncadeadas() {
		primeitroNo = null;
		ultimoNo = null;
		this.tamanhoLista = 0;
	}
	
	
	public int size() {
		return tamanhoLista.
	}
}
